{
  "name": "zipcodetw-demo",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "copy-data": "cp ../zipcodetw/data/* public/data/",
    "build": "bun build ./src/index.ts --outdir ./public",
    "start": "bunx serve ./public",
    "dev": "bun build ./src/index.ts --outdir ./public --watch"
  },
  "dependencies": {
    "zipcodetw": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
